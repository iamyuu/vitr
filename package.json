{
	"name": "vitr",
	"private": true,
	"version": "0.0.0",
	"author": "Muhammad Yusuf <yusuf@iamyuu.dev>",
	"packageManager": "pnpm@8.6.12",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --ignore-path .gitignore --fix",
		"format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,css,json}\" --ignore-path .gitignore",
		"type-check": "tsc --pretty --noEmit",
		"setup": "pnpm install && husky install"
	},
	"dependencies": {
		"@ebay/nice-modal-react": "1.2.10",
		"@radix-ui/react-alert-dialog": "1.0.4",
		"@radix-ui/react-dialog": "1.0.4",
		"@radix-ui/react-dropdown-menu": "2.0.5",
		"@radix-ui/react-slot": "1.0.2",
		"@radix-ui/react-tooltip": "1.0.6",
		"@tanstack/react-query": "4.32.6",
		"class-variance-authority": "0.7.0",
		"clsx": "2.0.0",
		"ky": "0.33.3",
		"lucide-react": "0.268.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "6.15.0",
		"sonner": "0.6.2",
		"tailwind-merge": "1.14.0",
		"tailwindcss-animate": "1.0.6",
		"zod": "3.22.1"
	},
	"devDependencies": {
		"@tanstack/react-query-devtools": "4.32.6",
		"@types/react": "18.2.20",
		"@types/react-dom": "18.2.7",
		"@vitejs/plugin-react-swc": "3.3.2",
		"autoprefixer": "10.4.15",
		"eslint": "8.47.0",
		"eslint-config-kentcdodds": "20.5.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-import-resolver-alias": "1.1.2",
		"eslint-import-resolver-typescript": "3.6.0",
		"eslint-plugin-import": "2.28.0",
		"eslint-plugin-jsx-a11y": "6.7.1",
		"eslint-plugin-prettier": "5.0.0",
		"eslint-plugin-react": "7.33.2",
		"husky": "8.0.3",
		"is-ci-cli": "2.2.0",
		"lint-staged": "14.0.0",
		"postcss": "8.4.28",
		"prettier": "3.0.2",
		"pretty-quick": "3.1.3",
		"tailwindcss": "3.3.3",
		"typescript": "5.1.6",
		"vite": "4.4.9"
	},
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": true,
		"endOfLine": "auto",
		"printWidth": 120,
		"semi": true,
		"singleQuote": true,
		"jsxSingleQuote": true,
		"trailingComma": "all",
		"useTabs": true
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "eslint --fix --cache --max-warnings 0",
		"*.{js,jsx,ts,tsx,css,json}": "pretty-quick --staged"
	}
}
